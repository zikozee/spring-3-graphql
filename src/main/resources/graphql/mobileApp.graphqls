type MobileApp {
    name: String!
    version: String
    platform: [String]
    author: Author
    appId: ID!
    releaseDate: Date
    downloaded: NonNegativeInt
    homepage: String
}

input MobileAppFilter {
    name: String
    version: String
    platform: String
    author: AuthorFilter
    releaseAfter: Date
    minimumDownload: NonNegativeInt
}

input AuthorFilter {
    name: String
}